{
  "compilerOptions": {
    "target": "ES2016",
    "module": "ES2020",
    "lib": [
      "ES2015"
    ],
    "downlevelIteration": true,
    "strict": true,
    "declaration": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": [
      "core-js"
    ],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "./node_modules"
  ],
  "files": [
    "allMatch.ts",
    "anyMatch.ts",
    "concat.ts",
    "dropWhile.ts",
    "filter.ts",
    "findFirst.ts",
    "flatMap.ts",
    "flatten.ts",
    "forEach.ts",
    "iterate.ts",
    "Iteration.ts",
    "limit.ts",
    "map.ts",
    "peek.ts",
    "range.ts",
    "rangeClosed.ts",
    "reduce.ts",
    "skip.ts",
    "takeWhile.ts",
    "toIterable.ts",
    "toIterator.ts",
    "zip.ts"
  ]
}
